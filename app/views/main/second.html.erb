<div class='container'>
    <% if !@results.any? %>
         <p>조회된 데이터 없음</p>
    <%end%>
    <%@results.each do |p| %>
    	<div class='col-lg-4 col-xs-12'>
    		<div class="panel panel-info"> 
    			<div class="panel-heading">
    				<%=@user.find(p.writer).name%>
    				<span style="float:right"><span style="color: gold;">★</span><%=p.avg_star_score%></span>
    			</div>
    			<div class="panel-body">
    			    <% if p.food_photo.to_s != "" %>
    				<a href="/read/<%=p.id%>"><img src="<%=p.food_photo%>"class="img-responsive"/> </a>
    			    <% else %>
    			    <a href="/read/<%=p.id%>"><img src="/rice.png"class="img-responsive"/> </a>
    			    <% end %>
    			</div> 
    			<ul class="list-group">
    				<li class="list-group-item"><a href="/read/<%=p.id%>" class="text-muted" style="font-size:20px"><%=p.name%></a></li>
    				<li class="list-group-item"><%=p.content%></li>
    				<li class="list-group-item"><%=p.final_date%> 까지</li> 
    			</ul>
    		</div>
    	</div>
	<%end%>
</div>